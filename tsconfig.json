{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "random/pornhub": [
        "libs/pornhub/src"
      ],
      "random/pornhub/*": [
        "libs/pornhub/src/*"
      ],
      "random/interfaces": [
        "libs/interfaces/src"
      ],
      "random/interfaces/*": [
        "libs/interfaces/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}